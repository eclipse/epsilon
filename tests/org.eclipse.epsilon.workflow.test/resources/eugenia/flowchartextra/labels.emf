@namespace(uri="epsilon/labels",prefix="epsl")
package labels;

enum LabelStyle {
   NORMAL;
   BOLD;
   ITALIC;
}

class Labels {
   val Label[*] labels;
}

class Label {
   attr String[1] name;
   !unique unsettable attr String[?] text;
   !unique unsettable attr LabelStyle[?] style;
}
