@test
operation testParallelSortByEquivalence() {
	var data : new Bag;
	data.add(0);
	data.add("");
	var i = 1073741824;
	while (i > 2) {
		i /= 2;
		data.add(i);
	}
	data.add("a loooooooooooooooooooong string");
	data.add(1);
	data.add(15);
	var expected = data.sortBy(n | n.asString().length());
	var actual = data.parallelSortBy(n | n.asString().length());
