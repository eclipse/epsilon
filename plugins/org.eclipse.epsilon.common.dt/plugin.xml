   </extension>
   <extension
         id="problemmarker"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
      <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.eclipse.epsilon.common.dt.preferences.EpsilonPreferencePage"
            id="Epsilon"
            name="Epsilon"/>
