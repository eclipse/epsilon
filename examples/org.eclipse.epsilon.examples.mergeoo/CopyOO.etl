rule CopyExternalClass
	transform s : Source!`ExternalClass`
	to t : Target!`ExternalClass` {
	
 
	t.`name` := s.`name`;
 
	t.`isAbstract` := s.`isAbstract`;
 
	t.`package` ::= s.`package`;
 
	t.`extends` ::= s.`extends`;
 
	t.`annotations` ::= s.`annotations`;
 
	t.`extendedBy` ::= s.`extendedBy`;
 
	t.`features` ::= s.`features`;
}
