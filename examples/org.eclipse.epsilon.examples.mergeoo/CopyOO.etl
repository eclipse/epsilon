rule CopyModel
	transform s : Source!`Model`
	to t : Target!`Model` {
	
 
	t.`name` := s.`name`;
 
	t.`package` ::= s.`package`;
 
	t.`annotations` ::= s.`annotations`;
 
	t.`contents` ::= s.`contents`;
}
rule CopyAnnotation
	transform s : Source!`Annotation`
	to t : Target!`Annotation` {
	
 
	t.`key` := s.`key`;
 
	t.`value` := s.`value`;
}
