rule MergeOperation
	merge l : Left!Operation
	with r : Right!Operation 
	into t : Target!Operation {
	
 
	t.name := l.name;
	t.visibility := l.visibility;
	t.owner ::= l.owner;
	t.type ::= l.type;
	t.annotations ::= l.annotations + r.annotations;
	t.parameters ::= l.parameters + r.parameters;
	
}
